  <title>tech@nyu</title> 
  <!--[if ! lte IE 8]><!-->
    <link rel="stylesheet" href="css/screen.css" type="text/css" />
    <link rel="stylesheet" href="css/skrollr-large.css" type="text/css" data-skrollr-stylesheet media="only screen and (min-width:920px) and (min-height: 620px) and (max-aspect-ratio: 1500/750)" />
    <link rel="stylesheet" href="css/skrollr-small.css" type="text/css" data-skrollr-stylesheet media="screen and (max-width:919px), (max-height: 620px), (min-aspect-ratio:1501/750)" />
