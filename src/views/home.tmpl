<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"> <!-- Force Latest IE rendering & Chrome Frame -->
  
  <meta name="Copyright" content="Copyright tech@nyu {{ now.getFullYear() }}." />
  <meta name="DC.creator" content="tech@nyu" />
  <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
  <link rel="icon" href="/favicon.ico" type="image/x-icon">

  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta name="HandheldFriendly" content="True" />
  <meta http-equiv="cleartype" content="on" />

  <title>tech@nyu</title> 
  <link rel="stylesheet" href="css/screen.css" type="text/css" />
  <link rel="stylesheet" href="css/skrollr.css" type="text/css" data-skrollr-stylesheet media="only screen and (min-width:768px)" />
  <script data-main="scripts/app" src="scripts/bower_components/requirejs/require.js"></script>
  <script type="text/javascript" src="//use.typekit.net/bpm7cfb.js"></script>
  <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
</head>
<body class="horiz">
  <header class="clearfix" id="header">
    <div class="info">
        <h1 class="logo">
        <a href="">
          {% include "partials/logo_with_alt.tmpl" %}
        </a>
        </h1>
      <p id="tagline" class="tagline">
        We host tech, design, and entrepreneurship events for students from NYU and surrounding schools. We have events for students of all experience levels.
      </p>
    </div>
    <a class="scrollCue" href="#getting-started">Skip to the Getting Started Section</a>
    <nav id="nav">
        <ol>
          {% for section in sections %}
            {% if not section.name|contains("Calendar") %} 
              <li class="h3">{{ section.name|add_br }}</li>
            {% else %}
              <li class="h3 calendar">{% include "partials/calendar_with_alt.tmpl" %}</li>
            {% endif %}
          {% endfor %}
        </ol>
    </nav>
  </header>
  <div id="upcoming">
    <div id="upcoming-inner">
      <div class="panel">
        <h2 class="h5" data-alt-title="Upcoming Events">Next Event</h2>

        <a class="event no-icon" href="" style="text-decoration:none;">
          <strong class="title">What Designers Actually Do</strong>
          <span>A Workshop with Anne-Laure Fayard <span style="text-decoration:underline;">More&nbsp;info&hellip;</span></span>
          <button class="flatButton">More Events</button>
        </a>

        <a class="event no-icon" href="" style="text-decoration:none;display:none;">
          <strong class="title">What Designers Actually Do</strong>
          <span>A Workshop with Anne-Laure Fayard <span style="text-decoration:underline;">More&nbsp;info&hellip;</span></span>
        </a>
        
      </div>

      <div class="panel follow">
        <h2 class="h5" style="vertical-align:top;">Notify Me of Future Events</h2>
        <div id="formWrapper">
          <input type="email" placeholder="Enter your email address..." /><input type="submit" value="Sign Up" />
        </div>
        <div class="instructions" style="display:none;">
          <p>Every other week, you&rsquo;ll get a digest of the best tech and entreprenuership events in NYC.</p>
        </div>
      </div>
   </div>
  </div>
  <div class="bg starter">
    <div class="img" data-wide="Tech@NYU_files/img/events/sqsp2.jpg" data-narrow="Tech@NYU_files/img/events/support2.jpg" /></div>
    <div class="coverup"><div class="mask"></div></div>
  </div>
  <div class="bg">
    <div class="img" data-wide="Tech@NYU_files/img/events/support1.jpg" data-narrow="Tech@NYU_files/img/events/support2.jpg" /></div>
  </div>
  <div class="bg">
    <div class="img" data-wide="Tech@NYU_files/img/events/support1.jpg" data-narrow="Tech@NYU_files/img/events/support2.jpg" /></div>
  </div>
  <div id="content">
    <div class="objective first" id="getting-started" data-section="1">
      <h2>Get Started with the Tech Scene</h2>
      <div class="initiative">
        <h3 class="h2">Freshman Circuit</h3>
        <p>Tour NYC startups and talk with their employees to see what they&rsquo;re up to and where you might fit in.</p>
        <br/>
        <h3>Next Event</h3>
        <h3>Meetup Info</h3>
      </div>

      <div class="initiative">
        <h3 class="h2">Programming &amp; Design Introductory Workshops</h3>
        <p>Learn the basics of programming or design.</p>
         <br/>
        <h3>Next Event</h3>
        <h3>Meetup Info</h3>
      </div>
    </div>

    <div class="objective second" id="improve-your-skills" data-section="2">
      <h2>Improve Your Skills</h2>
      <div class="initiative"> 
        <h3 class="h2">Hack Days</h3>
        <p>Lectures on web technologies, covering everything from <a href="http://angularjs.org">Angular</a> to <a href="http://zeptojs.com/">Zepto</a>.</p>
      </div>
      <div class="initiative">
        <h3 class="h2">Design Days</h3>
        <p>Workshops on design thinking, user research, and advanced topics in digital product design.</p>
      </div>
      <div class="initiative">
        <h3 class="h2">Game Days</h3>
        <p>Research, Special Topics, Design Thinking, and Practice Design Hackathons</p>
      </div>
    </div>

    <div class="objective third" id="build-and-socialize" data-section="3">      
      <h2>Build &amp; Socialize with Cool People</h2>
      <div class="initiative">
        <h3 class="h2">Hack Nights</h3>
        <p>Designers, developers, and makers of all sorts together or a few hours each week. Hang out, work on your projects and get to know each other.</p>
      </div>

      <div class="initiative">
        <h3 class="h2">Designathons</h3>
        <p>Over the course of a weekend, work with a team to produce a refined, well-researched design concept for a product of your choice.</p>
      </div>
    </div>

    <div class="objective fourth" id="latest-in-tech" data-section="4">
      <h2>Follow the Latest in Tech</h2>
      <div class="initiative">
        <h3 class="h2">Startup Series</h3>
        <p>Research, Special Topics, Design Thinking, and Practice Design Hackathons</p>
      </div>

      <div class="initiative">
        <h3 class="h2">Demo Days</h3>
        <p>Research, Special Topics, Design Thinking, and Practice Design Hackathons</p>
      </div>  

      <div class="initiative">
          <h3 class="h2">Digest</h3>
          <p>Our bi-weekly email of all the best tech &amp; design events around the city.</p>
          <input type="text" placeholder="Email Address" /><input type="submit" value="Subscribe" />
      </div>
    </div>

    <div class="objective fifth" id="about" data-section="5">
      <h2>About Us</h2>
      <div class="initiative">
        <h3 class="h2">Our Team</h3>
      </div>
      <div class="initiative">
        <h3>What We&rsquo;ve Made</h3>
      </div>

      <div class="initiative">
          <h3 class="h2">Contact</h3>
      </div>


      <div class="initiative">
        <h3 class="h2">Latest Blog Post</h3>
        <p>Blog Post</p>
        <p><a href="">Read more</a></p>
      </div>


      <div style="clear:left;margin-top:2em;overflow:auto;">
        <h2>Job Board</h2>
        <div class="initiative">
          <h3>Job Title 1</h3>
        </div>
        <div class="initiative">
          <h3>Job Title 2</h3>
          <p>Research, Special Topics, Design Thinking, and Practice Design Hackathons</p>
        </div>
        <div class="initiative">
            <h3>More Jobs...</h3>
        </div>
      </div>

      <div class="poweredby" style="clear:left;margin-top:2em;overflow:auto;">
        <h2>Supported By</h2>
        <div class="initiative"><h3><img src="./Tech@NYU_files/pivotal labs.png"></h3></div>
        <div class="initiative"><h3><img src="./Tech@NYU_files/pivotal labs.png"></h3></div>
        <div class="initiative"><h3><img src="./Tech@NYU_files/pivotal labs.png"></h3></div>
        <div class="initiative"><h3><img src="./Tech@NYU_files/pivotal labs.png"></h3></div>
        <div class="initiative"><h3><img src="./Tech@NYU_files/pivotal labs.png"></h3></div>
        <div class="initiative"><h3><img src="./Tech@NYU_files/pivotal labs.png"></h3></div>
      </div>
    </div>

    <div class="objective sixth" id="event-calendar" data-section="6">
      <h2>Event Calendar</h2>
      <p> This screen also needs to be designed. A sign up form for the digest could appear here too.</p>
    </div>
  </div>
  <script type="text/javascript" src="skrollr.stylesheets.min.js"></script>
  <script type="text/javascript">
    $(function() { 

      //add a dynamic animation setting to center our toc
      var toShiftTOC = Math.round(($(window).width() - 1320)/2),
          logoElm    = $('.logo a'),
          topLis     = $('nav > ul > li'),
          calendarSVGPaths = topLis.last().find('svg path'),
          $window = $(window),
          tocSubnavCascaded = false,
          sectionOneScrollPos = 400,
          firstScrollTime;

      /*if(toShiftTOC > 0) {
        $('nav').css('max-width', parseInt($('nav').css('max-width')) + toShiftTOC + 'px');
        $('nav > ul').css('margin-left', parseInt($('nav > ul').css('margin-left')) + toShiftTOC + 'px');
        //logoElm.attr('data-0', logoElm.attr('data-0')+'; padding-left:30px;display:block;');
        //logoElm.attr('data-525', logoElm.attr('data-525')+'; padding-left:'+ (toShiftTOC + 30) + 'px;');
      }*/

      /*$window.scroll(function(ev) {
        var scrollTop = $window.scrollTop();

        if(scrollTop > 390 && !tocSubnavCascaded) {
          $('nav ul ul').each(function(listIndex, list) {
              var list = $(list);
              list.animate({'top':'80px'}, 100);
              list.find('li').each(function(listItemIndex, item) {
                var item = $(item);
                setTimeout(function() { item.animate({'top':listItemIndex*40 + 'px'}, 200); }, 100*listItemIndex);
              })
          });

          tocSubnavCascaded = true;
        }

      });*/
      /*
      $window.scroll(function(event) {
        var scrollTop = $window.scrollTop(),
            now = new Date(),
            hash = window.location.hash;

        if(typeof firstScrollTime == 'undefined') {
          firstScrollTime = new Date();
        }

        if(scrollTop > sectionOneScrollPos && Math.abs(now.getTime() - firstScrollTime.getTime()) < 50 && hash == '') {
          $('html, body').animate({"scrollTop": sectionOneScrollPos}, Math.abs(scrollTop - sectionOneScrollPos)/10);
          event.preventDefault();
        }
      });
      */

      

      //active manager
      var calendarLastFill;
      $window.scroll(function(e) {
        var activeSection = $('.objective.skrollable-after + .objective').last(), 
            sectionNumber = activeSection.attr('data-section') || 1;
            activeLi      = topLis.filter(':nth-of-type('+sectionNumber+')');

        topLis.removeClass('active');
        activeLi.addClass('active');
        if(sectionNumber==6) {
          calendarLastFill = calendarLastFill || calendarSVGPaths.css('fill');
          calendarSVGPaths.css('fill', '');
        }
        else {
          calendarSVGPaths.css('fill', calendarLastFill);
        }
      });

      $('input[type=email]').focus(function(e) {
        $('.instructions').show();
        $('#tagline,.logo').animate({'margin-top':'-=2.5em'},140);
        $('#upcoming').animate({'height':'+=2.5em'},100);
      }).blur(function(e) {
        $('.instructions').hide();
        $('#upcoming,#tagline,.logo').animate({'margin-top':'+=2.5em'},140);
        $('#upcoming').animate({'height':'-=2.5em'},100);
      });

      var imgDiv =  $('.bg .img'), $window = $(window);
      
      function setBackgroundImg() {
        if($window.width() > 1270) {
          imgDiv.each(function() {
            var $this = $(this);
            $this.css('background-image', 'url(' + $this.attr('data-wide') + ')');
          });
        }
        else {
          imgDiv.each(function() {
            var $this = $(this);
            $this.css('background-image', 'url(' + $this.attr('data-narrow') + ')');
          });
        }
      }
      $(function(){
        $('.stroke-double, .stroke-single').attr('data-stroke', function(){
          return $(this).text();
        });
      });

      $window.resize(function(e) { setBackgroundImg(); });
      setBackgroundImg();
    });
  </script>
</body>
</html>
  